{
  "name": "webpack-javascript-template",
  "version": "1.0.0",
  "description": "A Javascript project template for creating JavaScript applications with ease.",
  "main": "index.js",
  "scripts": {
    "watch": "webpack serve --config webpack/webpack.dev.cjs",
    "build": "webpack build --config webpack/webpack.prod.cjs",
    "bundle": "npm run build && node bundle.cjs",
    "serve": "npm run build && serve dist",
    "format": "prettier --check public/scripts/**",
    "format:write": "prettier --write public/scripts/**"
  },
  "keywords": [
    "template",
    "javascript",
    "js",
    "webpack",
    "webpack5",
    "babel",
    "babeljs"
  ],
  "author": "Anthony I. Jackson Jr.",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@eslint/js": "^9.12.0",
    "archiver": "^7.0.1",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "node-tar.gz": "^1.0.0",
    "prettier": "^3.3.3",
    "serve": "^14.2.3",
    "style-loader": "^4.0.0",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1",
    "webpack-obfuscator": "^3.5.1",
    "zip-lib": "^1.0.4"
  }
}
